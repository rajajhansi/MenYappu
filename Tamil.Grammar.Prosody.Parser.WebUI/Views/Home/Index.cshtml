                    $("#about2Header").append(menyappuAnEffort);
                    var servicesTemplate = kendo.template($("#servicesTemplate").html());
                    $("#services").append(servicesTemplate({}));
                    Utility.wireDropdownTooltipAndPopoverHandlers();
                    // Initialize WOW.js Scrolling Animations
                    new WOW().init();

                    $('#carous-ss').owlCarousel({
                        items: 1,
                        autoPlay: 3000,
                        nav: true,
                        navText: [
                            '<i class="fa fa-chevron-left fa-3x wow bounceIn" style="visibility: visible; animation-name: bounceIn;"></i>',
                            '<i class="fa fa-chevron-right fa-3x wow bounceIn" style="visibility: visible; animation-name: bounceIn;"></i>'
