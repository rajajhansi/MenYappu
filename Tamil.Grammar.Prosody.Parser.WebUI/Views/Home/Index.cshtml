        <script>
            $(document)
                .ready(function() {
                    var landingTopNavTemplate = kendo.template($("#landingTopNavTemplate").html());
                    $("#landingTopNav").append(landingTopNavTemplate({}));
                    var landingHeaderTemplate = kendo.template($("#landingHeaderTemplate").html());
                    $("#landingHeader").append(landingHeaderTemplate({}));
                    var aboutSection = kendo.template($("#aboutSection").html());
                    $("#about").append(aboutSection({}));
